{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended", "plugin:import/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": ["error", "always"]
  }
}
